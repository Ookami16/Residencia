<div class="container">
    <%= render template: "siged/menuAdmin" %>
    <% breadcrumb :cat_tipo_participacion, @tipoParticipacion%>
    
    <h3 id="align-title">Datos del Tipo de Participación </h3>
    
    <dl class="dl-horizontal" id="align-list">
        <dt>Participación: </dt>
        <dd><%= @tipoParticipacion.participacion %></dd>
    </dl>

    <div class="row">
        <div class="col-xs-2 col-md-4" id="btn-cancelar">
            <%= button_to "Editar", edit_cat_tipo_participacion_path(@tipoParticipacion), :class => 'btn btn-success', :method => :get %>
        </div>
        <div class="col-xs-2 col-md-4" id="btn-cancelar">
            <%= button_to "Regresar", cat_tipo_participaciones_path, :class => 'btn btn-info', :method => :get %>
        </div>
    </div>
</div>