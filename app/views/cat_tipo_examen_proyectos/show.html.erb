<div class="contenedor">
    
    <% if current_usuario.rol == 'admin' %>
        <%= render template: "siged/menuAdmin" %>
    <% elsif current_usuario.rol == 'docente' %>
        <%= render template: "siged/menuDocente" %>
    <% end %>
    <% breadcrumb :cat_tipo_examen_proyecto, @tipoexproy%>

    <h3 id="align-title">Datos del Tipo de Examen Profesional / Proyecto</h3>
    
    <dl class="dl-horizontal" id="align-list">
        <dt>Tipo: </dt>
        <dd><%= @tipoexproy.nombreTipoP %></dd>
    </dl>
    
    <div class="row">
        <div class="col-xs-2 col-md-4" id="btn-cancelar">
            <%= button_to "Editar", edit_cat_tipo_examen_proyecto_path(@tipoexproy), :class => 'btn btn-success', :method => :get %>
        </div>
        <div class="col-xs-2 col-md-4" id="btn-cancelar">
            <%= button_to "Regresar", cat_tipo_examen_proyectos_path, :class => 'btn btn-info', :method => :get %>
        </div>
    </div>
</div>