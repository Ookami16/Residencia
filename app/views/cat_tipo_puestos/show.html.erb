<div class="container">
    <%= render template: "siged/menuAdmin" %>
    <% breadcrumb :cat_tipo_puesto, @tipoPuesto %>

    <h3 id="align-title">Datos del Tipo de Puesto</h3>
    
    <dl class="dl-horizontal" id="align-list">
        <dt>Nombre del puesto: </dt>
        <dd><%= @tipoPuesto.nombrePuesto %></dd>
        <dt>Alterno: </dt>
        <dd><%= @tipoPuesto.alterno %></dd>
    </dl>

    <div class="row">
        <div class="col-xs-2 col-md-4" id="btn-cancelar">
            <%= button_to "Editar", edit_cat_tipo_puesto_path(@tipoPuesto), :class => 'btn btn-success', :method => :get %>
        </div>
        <div class="col-xs-2 col-md-4" id="btn-cancelar">
            <%= button_to "Regresar", cat_tipo_puestos_path, :class => 'btn btn-info', :method => :get %>
        </div>
    </div>
</div>