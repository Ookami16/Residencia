<p id="notice"><%= notice %></p>

<h1>Listing Datos Generales Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre empresa</th>
      <th>Calle</th>
      <th>Numero</th>
      <th>Colonia</th>
      <th>Cp</th>
      <th>Ciudad</th>
      <th>Estado</th>
      <th>Municipio</th>
      <th>Telefono</th>
      <th>Correo</th>
      <th>Tipo empresa</th>
      <th>Tam empresa</th>
      <th>Actividad economica</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datos_generales_empresas.each do |datos_generales_empresa| %>
      <tr>
        <td><%= datos_generales_empresa.nombre_empresa %></td>
        <td><%= datos_generales_empresa.calle %></td>
        <td><%= datos_generales_empresa.numero %></td>
        <td><%= datos_generales_empresa.colonia %></td>
        <td><%= datos_generales_empresa.cp %></td>
        <td><%= datos_generales_empresa.ciudad %></td>
        <td><%= datos_generales_empresa.estado %></td>
        <td><%= datos_generales_empresa.municipio %></td>
        <td><%= datos_generales_empresa.telefono %></td>
        <td><%= datos_generales_empresa.correo %></td>
        <td><%= datos_generales_empresa.tipo_empresa %></td>
        <td><%= datos_generales_empresa.tam_empresa %></td>
        <td><%= datos_generales_empresa.actividad_economica %></td>
        <td><%= link_to 'Show', datos_generales_empresa %></td>
        <td><%= link_to 'Edit', edit_datos_generales_empresa_path(datos_generales_empresa) %></td>
        <td><%= link_to 'Destroy', datos_generales_empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

